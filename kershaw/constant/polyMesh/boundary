4
(
    movingWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          40;
        startFace       3120;
    }
    fixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          120;
        startFace       3160;
    }
    front
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          1600;
        startFace       3280;
    }
    back
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          1600;
        startFace       4880;
    }
