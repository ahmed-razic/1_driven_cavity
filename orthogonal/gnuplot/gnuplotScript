set terminal qt 0

set grid xtics
set grid ytics

set key left
set ylabel "Y centerline"
set xlabel "Ux"

plot 'gnuplot/UX_yline' u 2:1 w lp pt 7 title 'Ghia et. al.','postProcessing/sampleDict/10/l2.xy' u 2:1 w lp pt 6 title 'Current solution'


set terminal qt 1

set grid xtics
set grid ytics

set key right
set xlabel "X centerline"
set ylabel "Uy"

plot 'gnuplot/UY_xline' u 1:2 w lp pt 7 title 'Ghia et. al.','postProcessing/sampleDict/10/l1.xy' u 1:3 w lp pt 6 title 'Current solution'

print "Plotting with GNUPLOT"
print "Press ENTER or ctrl-c to exit"

pause -1

#EOF
