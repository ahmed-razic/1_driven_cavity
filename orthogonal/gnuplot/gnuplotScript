set terminal qt 0

set grid xtics
set grid ytics

set key left
set ylabel "Y centerline"
set xlabel "Ux"

plot 'gnuplot/UX_yline' u 2:1 w lp pt 7 title 'Ghia et. al.', \
'postProcessing/sampleDict/50/l2.xy' u 2:1 w lp pt 6 title 'Current solution'


set terminal qt 1

set grid xtics
set grid ytics

set key left
set ylabel "X centerline"
set xlabel "Uy"

plot 'gnuplot/UY_xline' u 2:1 w lp pt 7 title 'Ghia et. al.', \
'postProcessing/sampleDict/50/l1.xy' u 2:1 w lp pt 6 title 'Current solution'

print "Plotting with GNUPLOT"
print "Press ENTER or ctrl-c to exit"

pause -1

